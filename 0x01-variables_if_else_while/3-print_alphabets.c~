#include <stdio.h>
#include <stdlib.h>
/**                                                                             *main - out lowercase letters                                                  *                                                                              *Return: return 0 on success                                                   */
int main(void)
{
  /* outpot lowercase */
char ch;
ch = 'a';
while (ch <= 'z')
{
putchar(ch);
++ch;
}
ch = 'A';
while (ch <= 'Z')
{
putchar(ch);
++ch;
}
putchar('\n');
return (0)
}
